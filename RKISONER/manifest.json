{
  "manifest_version": 3,
  "name": "Rkisoner",
  "version": "1.0",
  "description": "Blur YouTube content by custom keywords. (GTA VI Inspired)",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["save.mp3"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options.html"
}
